#include<bits/stdc++.h>

using namespace std;

int main(){
    string s;
    std::getline(cin,s);
    for(int i=0;i<s.length();i++){
        if(s[i]==' ')
            continue;
        s[i]=s[i]-3;
        if((s[i]<97 && s[i]>90) || s[i]<65){
            s[i]+=26;
        }
    }
    cout<<s;

    return 0;
}

